{
  "name": "calorie-counter-server",
  "version": "1.0.0",
  "description": "Backend server for Calorie Counter app",
  "main": "server.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "client": "cd client && npm start",
    "build": "npm install && cd client && npm install &&npm run build",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\""
  },
  "dependencies": {
    "@google-cloud/vision": "^5.2.0",
    "@tailwindcss/postcss": "^4.1.11",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "react-scripts": "^5.0.1",
    "web-vitals": "^5.0.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.10"
  }
}
